    "moment": "^2.22.2",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-dom": "^16.4.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "concurrently \"react-scripts start\" \"json-server --watch sample-data.json --port 3001\"",
    "build": "react-scripts build",
